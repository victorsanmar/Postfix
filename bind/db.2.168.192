;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     servidor.victorasir.com. root.victorasir.com. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;

@       IN      NS      servidor.victorasir.com.
2       IN      PTR     servidor.victorasir.com.
51      IN      PTR     cliente1.victorasir.com.
1       IN      PTR     router.victorasir.com.
52      IN      PTR     cliente2.victorasir.com.
2       IN      PTR     mail.victorasir.com.
